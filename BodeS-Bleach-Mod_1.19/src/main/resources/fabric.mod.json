{
  "schemaVersion": 1,
  "id": "bleachmod",
  "version": "${version}",

  "name": "Bode's Bleach Mod",
  "description": "This is an mod created by Bode (Bodewilson) inspired in Bleach 'anime'",
  "authors": [
    "Bodewilson"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT License",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.bodewilson.bleachmod.BleachMod"
    ],
    "client": [
      "net.bodewilson.bleachmod.BleachModClient"
    ]
  },
  "mixins": [
    "bleachmod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
